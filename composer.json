{
    "name": "silverd/laravel-apn-notification-channel",
    "description": "Apple Push Notification Service (APNs) notifications channel for Laravel 6 using the new APNs HTTP/2 protocol with token-based (JWT with p8 private key)",
    "keywords": [
        "apns",
        "push notifications",
        "p8",
        "php apns",
        "laravel apns",
        "laravel p8"
    ],
    "homepage": "https://github.com/silverd/laravel-apn-notification-channel",
    "license": "MIT",
    "authors": [
        {
            "name": "Semyon Chetvertnyh",
            "email": "semyon.chetvertnyh@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "edamov/pushok": "^0.10",
        "illuminate/notifications": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "squizlabs/php_codesniffer": "^3.4",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "SemyonChetvertnyh\\ApnNotificationChannel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SemyonChetvertnyh\\ApnNotificationChannel\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SemyonChetvertnyh\\ApnNotificationChannel\\ApnServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
